<h1>Listing dayboards</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @dayboards.each do |dayboard| %>
  <tr>
    <td><%= dayboard.name %></td>
    <td><%= link_to 'Show', dayboard %></td>
    <td><%= link_to 'Edit', edit_dayboard_path(dayboard) %></td>
    <td><%= link_to 'Destroy', dayboard, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Dayboard', new_dayboard_path %>
